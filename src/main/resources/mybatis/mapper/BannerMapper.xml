<mapper namespace="com.zcf.zhenxuan.mapper.BannerMapper">


    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.zcf.zhenxuan.bean.Banner">
        <result column="bannerid" property="bannerid"/>
